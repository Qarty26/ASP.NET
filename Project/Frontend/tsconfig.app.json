{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue",
    "src/**/*.ts",
    "formkit.config.ts",
    "formkit.theme.ts",
    "src/helpers/AxiosInstance.ts",
    "src/helpers/Authenticated.ts",
    "src/helpers/*.ts",
    "src/models/*.ts",
    "src/models/*.ts",
    "src/models/**/*.ts",
    "src/helpers/**/*.ts",
    "src/utils/*.ts"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}